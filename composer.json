{
    "name": "mahdian_manesh/laravel-test-generator",
    "description": "Laravel package for generating unit test automatically",
    "license": "MIT",
    "authors": [
        {
            "name": "Hassan Mahdian Manesh",
            "email": "mehdian92@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "fakerphp/faker": "^1.23.0"
    },
    "autoload": {
        "psr-4": {
            "Mahdianmanesh\\LaravelTestGenerator\\": "src/"
        }
